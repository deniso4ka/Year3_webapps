{% extends '_baseAdmin.html.twig' %}

{# ----------------------------------------#}
{% block title %}
    Member Publication Create Page
{% endblock %}

{# ----------------------------------------#}
{% block homeNavStyle %}current_page{% endblock %}

{# ----------------------------------------#}
{% block main %}
<h4>{{ message }}</h4>
    <table>
        <tr>
            <th> ID </th>
            <th> Name </th>
            <th> Supervisor Id </th>
            <th> Project Description </th>
            <th> Status </th>
            <th> Image </th>
            <th> </th>

        </tr>


        {% for project in projects %}
            <tr>
                <td>{{ project.id }}</td>
                <td>{{ project.name }}</td>
                <td>{{ project.supervisor }}</td>
                <td>{{ project.description }}</td>
                <td>{{ project.status }}</td>
                <td>{{ project.image }}</td>
                <td><a href="adminDeleteProject/{{ project.id }}">{{ delete  }}</a></td>
            </tr>
        {% endfor %}

    </table>








{% endblock %}
