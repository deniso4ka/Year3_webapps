{% extends '_baseAdmin.html.twig' %}

{# ----------------------------------------#}
{% block title %}
    Member Publication Create Page
{% endblock %}

{# ----------------------------------------#}
{% block homeNavStyle %}current_page{% endblock %}

{# ----------------------------------------#}
{% block main %}

    <table>
        <tr>
            <th> ID </th>
            <th> Name </th>
            <th> Project Id </th>
            <th> Status </th>
            <th> Image </th>

{{ members.id }}

        </tr>

        {% for member in members %}
            <tr>
                <td>{{ member.id }}</td>
                <td>{{ member.name }}</td>
                <td>{{ member.projectId }}</td>
                <td>{{ member.status }}</td>
                <td>{{ member.image }}</td>
                <td><a href="adminDeleteMember/{{ member.id }}">{{ delete  }}</a></td>
            </tr>
        {% endfor %}

    </table>








{% endblock %}
