{% extends '_base.html.twig' %}

{# ----------------------------------------#}
{% block title %}
    Home page
{% endblock %}

{# ----------------------------------------#}


{# ----------------------------------------#}
{% block main %}

    <dl>
        <dt>Id</dt>
        <dd>{{ project.id }}</dd>

        <dt>Name</dt>
        <dd>{{ project.name }}</dd>

        <dt>Supervisor</dt>
        <dd> Id is :<b> {{ project.supervisor }}</b> and name is <b>{{ supervisorName }}</b></dd>

        <dt>Description</dt>
        <dd>{{ project.description }}</dd>

        <dt>Status</dt>
        <dd>{{ project.status }}</dd>

        <img class="img-circle" src="../images/{{ project.image }}" alt="image" style="width:150px; height:170px;">

    </dl>

    <p>-----------------------------------------</p>
    <h3>{{ studentsDetails }}</h3>

{% for student in studentRow %}
    <p>{{ textId }}<b> {{ student.id }}</b></p>
    <p>{{ textName }}<b> {{ student.name }}</b></p>
    {% endfor %}

{% endblock %}


