{% extends '_base.html.twig' %}

{# ----------------------------------------#}
{% block title %}
    Home page
{% endblock %}

{# ----------------------------------------#}


{# ----------------------------------------#}
{% block main %}

    <dl>
        <dt>Id</dt>
        <dd>{{ member.id }}</dd>

        <dt>Name</dt>
        <dd>{{ member.name }}</dd>

        <dt>Project Id</dt>
        <dd>{{ member.projectId }}</dd>

        <dt>Status</dt>
        <dd>{{ member.status }}</dd>


        <dt>Image</dt>
        <dd> <img class="img-circle" src="/images/{{ member.image }}" alt="image" style="width:150px; height:170px;"></dd>
    </dl>

    <p>--------------------------------------</p>
    <h3>{{ studentsDetails }}</h3>

    {% for student in students %}


            {{ textId }} {{ student.id }}<br>
            {{ textName }} {{ student.name }}<br>

    {% endfor %}
    <p>--------------------------------------</p>
    <h3>Project Details</h3>
            Project name is : {{ projectName }}<br>


{% endblock %}


